# Uncomment this line to define a global platform for your project
 platform :ios, '10.0'

target 'TMPlus' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TMPlus
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  pod 'Google/SignIn'
  pod 'Fabric'
  pod 'TwitterKit'
  target 'TMPlusTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TMPlusUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
